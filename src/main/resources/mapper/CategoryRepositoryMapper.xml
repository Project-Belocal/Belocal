<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.belocal.web.repository.CategoryRepository">

    <select id="findAllCtg" resultType="Category">
        SELECT *
        FROM
        category
        ORDER BY id ASC
    </select>


