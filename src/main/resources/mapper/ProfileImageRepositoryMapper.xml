<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.belocal.web.repository.ProfileImageRepository">
    <select id="findByMemberId" resultType="ProfileImage">
        SELECT *
        FROM profile_image
        WHERE member_id = #{memberId}
    </select>
</mapper>